<button class="auth-button oauth-{{classSuffix}}" title="Login with {{name}}">
  <a href="{{url}}" {{#if popup}}($click)="openPopup(%event, url)"{{/if}} >
    {{#if svg}}
      <span class="SVGInline">
        {{{svg}}}
      </span>
    {{else}}
      <img class='logo' src="{{img}}" alt="{{alt}}" />
    {{/if}}

    {{#if text}}
      <span class="auth-button-text">{{text}}</span>
    {{/if}}
  </a>
</button>
